@prefix : <http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#> .

<http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom> rdf:type owl:Ontology ;
                                                                       <http://purl.org/dc/terms/license> "http://purl.org/NET/rdflicense/cc-by4.0" ;
                                                                       rdfs:comment "Ontology about the TV Show \"The Last Kingdom\" created by Eduardo Ruiz Avia" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#ancestor_of
:ancestor_of rdf:type owl:ObjectProperty ;
             owl:inverseOf :has_ancestor ;
             rdfs:domain :Person ;
             rdfs:range :Person ;
             rdfs:comment "Person whose instance is ancestor" ;
             rdfs:label "ancestor_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#belongs_to
:belongs_to rdf:type owl:ObjectProperty ;
            owl:inverseOf :has_owner ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Location ;
            rdfs:range :Faction ;
            rdfs:label "Faction owner of the location" ,
                       "belongs_to" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_ancestor
:has_ancestor rdf:type owl:ObjectProperty ;
              rdfs:domain :Person ;
              rdfs:range :Person ;
              rdfs:comment "Person who is ancestor of the instance" ;
              rdfs:label "has_ancestor" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_beenSieged
:has_beenSieged rdf:type owl:ObjectProperty ;
                owl:inverseOf :sieged_in ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Siege ;
                rdfs:range :Fortress ;
                rdfs:comment "Fortress that has been sieged" ;
                rdfs:label "has_beenSieged" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_foughtWith
:has_foughtWith rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain :Faction ;
                rdfs:range :Faction ;
                rdfs:comment "Factions that fought for or againts the actual faction" ;
                rdfs:label "has_foughtWith" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_frontier
:has_frontier rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain :Faction ;
              rdfs:range :Faction ;
              rdfs:comment "Factions that border the current" ;
              rdfs:label "has_frontier" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_king
:has_king rdf:type owl:ObjectProperty ;
          owl:inverseOf :king_of ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Faction ;
          rdfs:range :King ;
          rdfs:comment "KIng that rules the faction" ;
          rdfs:label "has_king" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_lootedBy
:has_lootedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :looter_of ;
              rdfs:domain :Raid ;
              rdfs:range :Faction ;
              rdfs:comment "Faction that was looter in the raid" ;
              rdfs:label "has_lootedBy" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_member
:has_member rdf:type owl:ObjectProperty ;
            owl:inverseOf :member_of ;
            rdfs:domain :Faction ;
            rdfs:range :Person ;
            rdfs:comment "People who are members of the faction" ;
            rdfs:label "has_member" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_owner
:has_owner rdf:type owl:ObjectProperty ;
           rdfs:domain :Faction ;
           rdfs:range :Location ;
           rdfs:label "Location owned by the faction" ,
                      "has_owner" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_participant
:has_participant rdf:type owl:ObjectProperty ;
                 owl:inverseOf :participant_of ;
                 rdfs:domain :Battle ;
                 rdfs:range :Faction ;
                 rdfs:label "Faction that participated in the battle" ,
                            "has_participant" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_preferedWeapon
:has_preferedWeapon rdf:type owl:ObjectProperty ;
                    owl:inverseOf :preferedWeapon_of ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Noble
                                                :Warrior
                                              )
                                ] ;
                    rdfs:range :Weapon ;
                    rdfs:comment "Weapon prefered by nobles" ;
                    rdfs:label "has_preferedWeapon" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_religion
:has_religion rdf:type owl:ObjectProperty ;
              owl:inverseOf :religion_of ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Faction ;
              rdfs:range :Religion ;
              rdfs:comment "Religion of the faction" ;
              rdfs:label "has_religion" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_signatory
:has_signatory rdf:type owl:ObjectProperty ;
               owl:inverseOf :signatory_of ;
               rdfs:domain :PeaceTreaty ;
               rdfs:range :Faction ;
               rdfs:comment "Faction that signed the peace treaty" ;
               rdfs:label "has_signatory" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_victim
:has_victim rdf:type owl:ObjectProperty ;
            owl:inverseOf :victim_of ;
            rdfs:domain :Raid ;
            rdfs:range :Faction ;
            rdfs:comment "Faction that was looted in the raid" ;
            rdfs:label "has_victim" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#has_weapon
:has_weapon rdf:type owl:ObjectProperty ;
            owl:inverseOf :weapon_of ;
            rdfs:domain :Vassal ;
            rdfs:range :CheapWeapon ;
            rdfs:comment "Weapon used by vassals (the cheap ones)" ;
            rdfs:label "has_weapon" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#king_of
:king_of rdf:type owl:ObjectProperty ;
         rdfs:domain :King ;
         rdfs:range :Faction ;
         rdfs:comment "Faction ruled by the king" ;
         rdfs:label "king_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#looter_of
:looter_of rdf:type owl:ObjectProperty ;
           rdfs:domain :Faction ;
           rdfs:range :Raid ;
           rdfs:comment "Raid that was carried out by the faction" ;
           rdfs:label "looter_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#member_of
:member_of rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Person ;
           rdfs:range :Faction ;
           rdfs:comment "Faction the person is a member of" ;
           rdfs:label "member_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#participant_of
:participant_of rdf:type owl:ObjectProperty ;
                rdfs:domain :Faction ;
                rdfs:range :Battle ;
                rdfs:label "Battle in which the faction participated" ,
                           "participant_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#preferedWeapon_of
:preferedWeapon_of rdf:type owl:ObjectProperty ;
                   rdfs:domain :Weapon ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( :Noble
                                              :Warrior
                                            )
                              ] ;
                   rdfs:comment "Person whose instance is a prefered weapon" ;
                   rdfs:label "preferedWeapon_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#religion_of
:religion_of rdf:type owl:ObjectProperty ;
             rdfs:domain :Religion ;
             rdfs:range :Faction ;
             rdfs:comment "Faction that believes in this religion" ;
             rdfs:label "religion_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#sieged_in
:sieged_in rdf:type owl:ObjectProperty ;
           rdfs:domain :Fortress ;
           rdfs:range :Siege ;
           rdfs:comment "Siege in wich the fortress was attacked" ;
           rdfs:label "sieged_in" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#signatory_of
:signatory_of rdf:type owl:ObjectProperty ;
              rdfs:domain :Faction ;
              rdfs:range :PeaceTreaty ;
              rdfs:comment "Peace treaty that was signed by the faction" ;
              rdfs:label "signatory_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#victim_of
:victim_of rdf:type owl:ObjectProperty ;
           rdfs:domain :Faction ;
           rdfs:range :Raid ;
           rdfs:comment "Raid in which the faction was looted." ;
           rdfs:label "victim_of" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#weapon_of
:weapon_of rdf:type owl:ObjectProperty ;
           rdfs:domain :CheapWeapon ;
           rdfs:range :Vassal ;
           rdfs:comment "Person whose weapon is used" ;
           rdfs:label "weapon_of" .


###  http://www.w3.org/2002/07/owl#differentFrom
owl:differentFrom rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#age
:age rdf:type owl:DatatypeProperty ,
              owl:FunctionalProperty ;
     rdfs:domain :Person ;
     rdfs:range xsd:positiveInteger ;
     rdfs:comment "The age of someone" ;
     rdfs:label "age" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#expensive
:expensive rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Weapon ;
           rdfs:range xsd:boolean ;
           rdfs:comment "States the relative value of the weapon" ;
           rdfs:label "expensive" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Event
                                  :Faction
                                  :Location
                                  :Person
                                  :Religion
                                  :Weapon
                                )
                  ] ;
      rdfs:range xsd:string ;
      rdfs:comment "Name of something or someone" ;
      rdfs:label "name" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#nickname
:nickname rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Person ;
          rdfs:range xsd:string ;
          rdfs:comment "The nickname of someone" ;
          rdfs:label "nickname" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#population
:population rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Location ;
            rdfs:range xsd:positiveInteger ;
            rdfs:comment "Approximate population of a place" ;
            rdfs:label "population" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#year
:year rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :Event ;
      rdfs:range xsd:positiveInteger ;
      rdfs:comment "Year of the event" ;
      rdfs:label "year" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#AngloDanes
:AngloDanes rdf:type owl:Class ;
            rdfs:subClassOf :Faction ;
            rdfs:comment "Danish factions settled in England" ;
            rdfs:label "AngloDanes" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#AngloSaxons
:AngloSaxons rdf:type owl:Class ;
             rdfs:subClassOf :Faction ;
             rdfs:comment "Saxon factions settled in England" ;
             rdfs:label "AngloSaxons" ;
             rdfs:sameAs "https://dbpedia.org/page/Anglo-Saxons" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Battle
:Battle rdf:type owl:Class ;
        rdfs:subClassOf :Event ;
        rdfs:comment "A battle that has occurred in the series" ;
        rdfs:label "Battle" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Belligerent
:Belligerent rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:unionOf ( [ owl:intersectionOf ( :Faction
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :participant_of ;
                                                                          owl:someValuesFrom :Battle
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :looter_of ;
                                                   owl:someValuesFrom :Raid
                                                 ]
                                               )
                                 ] ;
             rdfs:subClassOf :Faction ;
             rdfs:comment "Factions that took part in any battle or raid" ;
             rdfs:label "Belligerent" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Britons
:Britons rdf:type owl:Class ;
         rdfs:subClassOf :Faction ;
         rdfs:comment "The Welsh are the descendants of the ancient Britons, pushed west and north of the island by the Saxons in the 5th and 7th centuries." ;
         rdfs:label "Britons" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Champion
:Champion rdf:type owl:Class ;
          rdfs:subClassOf :Warrior ;
          rdfs:comment "Specialized warriors who used to be part of the elite forces of the time" ;
          rdfs:label "Champion" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#CheapWeapon
:CheapWeapon rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Weapon
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :expensive ;
                                                          owl:hasValue "false"^^xsd:boolean
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Weapon ;
             rdfs:comment "Weapons that are relatively cheap" ;
             rdfs:label "CheapWeapon" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#ChristianFaction
:ChristianFaction rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Faction
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :has_religion ;
                                                               owl:someValuesFrom :Christianism
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:comment "Factions whose religion is Christianism" ;
                  rdfs:label "ChistianFaction" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Christianism
:Christianism rdf:type owl:Class ;
              rdfs:subClassOf :Religion ;
              owl:disjointWith :Paganism .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Location ;
      rdfs:comment "Population center" ;
      rdfs:label "City" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Clergy
:Clergy rdf:type owl:Class ;
        rdfs:subClassOf :Vassal ;
        rdfs:comment "Member of the clergy" ;
        rdfs:label "Clergy" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Event
:Event rdf:type owl:Class ;
       rdfs:comment "An important event that happened in the series" ;
       rdfs:label "Event" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Faction
:Faction rdf:type owl:Class ;
         rdfs:comment "One of the factions of the time in the British Isles." ;
         rdfs:label "Faction" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Fortress
:Fortress rdf:type owl:Class ;
          rdfs:subClassOf :City ;
          rdfs:comment "Fortified population center" ;
          rdfs:label "Fortress" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Irish
:Irish rdf:type owl:Class ;
       rdfs:subClassOf :Faction ;
       rdfs:comment "Christianized people of Ireland" ;
       rdfs:label "Irish" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#King
:King rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :king_of ;
                            owl:someValuesFrom :Faction
                          ] ;
      rdfs:subClassOf :Witenagemot ;
      rdfs:comment "The King or the Jarl of the faction" ;
      rdfs:label "King" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Location
:Location rdf:type owl:Class ;
          rdfs:comment "Any type of location or territory" ;
          rdfs:label "Location" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#MeleeWeapon
:MeleeWeapon rdf:type owl:Class ;
             rdfs:subClassOf :Weapon ;
             rdfs:comment "Weapons used in melee or close combat" ;
             rdfs:label "MeleeWeapon" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Merchant
:Merchant rdf:type owl:Class ;
          rdfs:subClassOf :Vassal ;
          rdfs:comment "A buyer and seller of commodities for profit" ;
          rdfs:label "Merchant" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Noble
:Noble rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       owl:disjointWith :Vassal ;
       rdfs:label "Aristocratic people" ,
                  "Noble" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#NorseGaels
:NorseGaels rdf:type owl:Class ;
            rdfs:subClassOf :Faction ;
            rdfs:comment "Scandinavian population from Viking raids who settled in Ireland, Scotland, Wales" ;
            rdfs:label "NorseGaels" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#OldEnoughToFight
:OldEnoughToFight rdf:type owl:Class ;
                  rdfs:subClassOf :Person ;
                  rdfs:comment "People that are too old to fight" ;
                  rdfs:label "OldEnoughToFight" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#OneHanded
:OneHanded rdf:type owl:Class ;
           rdfs:subClassOf :MeleeWeapon ;
           rdfs:comment "Weapons that can be used with one hand" ;
           rdfs:label "OneHanded" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#PaganFaction
:PaganFaction rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Faction
                                                         [ rdf:type owl:Class ;
                                                           owl:complementOf :ChristianFaction
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Factions whose religion is not Christianism" ;
              rdfs:label "PaganFaction" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Paganism
:Paganism rdf:type owl:Class ;
          rdfs:subClassOf :Religion ;
          rdfs:comment "Any non-Christian religion" ;
          rdfs:label "Paganism" ;
          rdfs:sameAs "https://dbpedia.org/page/Paganism" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#PeaceTreaty
:PeaceTreaty rdf:type owl:Class ;
             rdfs:subClassOf :Event ;
             rdfs:comment "A peace treaty signed by 2 or more factions" ;
             rdfs:label "PeaceTreaty" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Peasant
:Peasant rdf:type owl:Class ;
         rdfs:subClassOf :Vassal ;
         rdfs:label "Peasant" ,
                    "Persons tilling the soil as small landowners or as laborers" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Person
:Person rdf:type owl:Class ;
        rdfs:comment "Any kind of person" ;
        rdfs:label "Person" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Raid
:Raid rdf:type owl:Class ;
      rdfs:subClassOf :Event ;
      rdfs:comment "A raid by one or more factions on a territory" ;
      rdfs:label "Raid" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#RangeWeapon
:RangeWeapon rdf:type owl:Class ;
             rdfs:subClassOf :Weapon ;
             rdfs:comment "Ranged weapons" ;
             rdfs:label "RangeWeapon" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Religion
:Religion rdf:type owl:Class ;
          rdfs:comment "Creed or belief" ;
          rdfs:label "Religion" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Scots
:Scots rdf:type owl:Class ;
       rdfs:subClassOf :Faction ;
       rdfs:comment "Celtic and Pictish settlers from the northern British Isles" ;
       rdfs:label "Scots" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Siege
:Siege rdf:type owl:Class ;
       rdfs:subClassOf :Battle ;
       rdfs:comment "A siege of a fortress that has occurred in the series" ;
       rdfs:label "Siege" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#TooOldToFight
:TooOldToFight rdf:type owl:Class ;
               rdfs:subClassOf :Person ;
               rdfs:comment "People that aren't too old to fight" ;
               rdfs:label "TooOldToFight" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#TwoHanded
:TwoHanded rdf:type owl:Class ;
           rdfs:subClassOf :MeleeWeapon ;
           rdfs:comment "Weapons that must be used with two hands" ;
           rdfs:label "TwoHanded" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Vassal
:Vassal rdf:type owl:Class ;
        rdfs:subClassOf :Person ;
        rdfs:comment "A holder of land by feudal tenure on conditions of homage and allegiance." ;
        rdfs:label "Vassal" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Vikings
:Vikings rdf:type owl:Class ;
         rdfs:subClassOf :Faction ;
         rdfs:comment "Peoples from Scandinavia, always willing to raid for slaves, booty and land." ;
         rdfs:label "Vikings" .


###  http://www.semanticweb.org/eduardo/ontologies/2022/3/thelastkingdom#Warrior
:Warrior rdf:type owl:Class ;
         rdfs:subClassOf :Vassal ;
